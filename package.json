{
  "name": "@moss/sdk",
  "version": "0.1.0",
  "description": "MOSS SDK for TypeScript - Cryptographic signing for AI agent outputs",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "moss",
    "ai",
    "agents",
    "signing",
    "cryptography",
    "post-quantum",
    "ml-dsa",
    "dilithium",
    "attribution",
    "audit"
  ],
  "author": "IAMPASS Inc.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mosscomputing/moss-sdk-ts"
  },
  "homepage": "https://mosscomputing.com",
  "bugs": {
    "url": "https://github.com/mosscomputing/moss-sdk-ts/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0",
    "eslint": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0"
  },
  "dependencies": {
    "@noble/hashes": "^1.3.0"
  }
}
